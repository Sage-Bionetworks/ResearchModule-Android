{
  "identifier": "PlaquesBodyMap",
  "steps": [
    {
      "identifier": "introduction",
      "type": "overview",
      "title": "Body plaque coverage",
      "text": "Select all areas on your body that currently have plaques.",
      "actions": {
        "goForward": {
          "type": "base",
          "buttonTitle": "Begin"
        }
      }
    },
    {
      "identifier": "bodySelection",
      "type": "bodySelection",
      "title": "Select areas on your body that currently have plaques.",
      "inputFields": [
        {
          "uiHint": "list",
          "dataType": "multipleChoice.string",
          "choices": [
            {
              "text": "Front of upper body.",
              "value": "frontUpper"
            },
            {
              "text": "Front of lower body.",
              "value": "frontLower"
            },
            {
              "text": "Back of upper body.",
              "value": "backUpper"
            },
            {
              "text": "Back of lower body.",
              "value": "backLower"
            },
            {
              "text": "None",
              "value": "none"
            }
          ]
        }
      ],
      "actions": {
        "goForward": {
          "type": "base",
          "buttonTitle": "Continue"
        }
      }
    },
    {
      "identifier": "frontUpper",
      "type": "plaqueBodyMap",
      "title": "Front of the upper body",
      "text": "Draw areas that have plaques",
      "actions": {
        "goForward": {
          "type": "base",
          "buttonTitle": "Next"
        }
      },
      "image": {
        "type": "fetchable",
        "imageName": "srpm_upper_front_body",
        "placementType": "fullsizeBackground"
      }
    },
    {
      "identifier": "frontLower",
      "type": "plaqueBodyMap",
      "title": "Front of the lower body",
      "text": "Draw areas that have plaques",
      "actions": {
        "goForward": {
          "type": "base",
          "buttonTitle": "Next"
        }
      },
      "image": {
        "type": "fetchable",
        "imageName": "srpm_lower_front_body",
        "placementType": "fullsizeBackground"
      }
    },
    {
      "identifier": "backUpper",
      "type": "plaqueBodyMap",
      "title": "Back of the upper body",
      "text": "Draw areas that have plaques",
      "actions": {
        "goForward": {
          "type": "base",
          "buttonTitle": "Next"
        }
      },
      "image": {
        "type": "fetchable",
        "imageName": "srpm_upper_back_body",
        "placementType": "fullsizeBackground"
      }
    },
    {
      "identifier": "backLower",
      "type": "plaqueBodyMap",
      "title": "Back of the lower body",
      "text": "Draw areas that have plaques",
      "actions": {
        "goForward": {
          "type": "base",
          "buttonTitle": "Next"
        }
      },
      "image": {
        "type": "fetchable",
        "imageName": "srpm_lower_back_body",
        "placementType": "fullsizeBackground"
      }
    }
  ]
}