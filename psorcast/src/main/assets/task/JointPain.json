{
  "identifier": "JointPain",
  "steps": [
    {
      "identifier": "introduction",
      "type": "overview",
      "title": "Painful Joint Count",
      "text": "In this activity you will first select the areas of your body with painful joints, then you will select the individual joints in those areas.",
      "actions": {
        "goForward": {
          "type": "base",
          "buttonTitle": "Get started"
        },
        "skip": {
          "type": "reminder",
          "reminderIdentifier": "JointPain"
        }
      },
      "image": {
        "type": "fetchable",
        "imageName": "srpm_joint_counting_intro",
        "placementType": "topBackground"
      }
    },
    {
      "identifier": "selectAreas",
      "type": "instruction",
      "title": "Select areas, then joints",
      "text": "Select areas of your body that have painful joints, then tap on the individual joints in those areas that are painful or tender.",
      "actions": {
        "goForward": {
          "type": "default",
          "buttonTitle": "Begin"
        }
      },
      "image": {
        "type": "animated",
        "imageNames": ["srpm_select_areas_joints1", "srpm_select_areas_joints2", "srpm_select_areas_joints3", "srpm_select_areas_joints4", "srpm_select_areas_joints5", "srpm_select_areas_joints6"],
        "placementType": "topBackground",
        "animationDuration": 3.0
      }
    },
